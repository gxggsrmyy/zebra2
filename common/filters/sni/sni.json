{
    "SNIServers": [
        // "1.2.3.4", "5.6.7.8",
    ],
    "RedirectServerName": {
        // "xxx.yyy.zzz": "yyy.zzz",
    },
    "Transport": {
        "Dialer": {
            "SocketReadBuffer": 0,
            "KeepAlive": 30,
            "Level": 8,
            "Timeout": 10
        },
        "Proxy": {
            "Enabled": false,
            "URL": "socks5://127.0.0.1:1080"
        },
        "DisableKeepAlives": false,
        "IdleConnTimeout": 60,
        "MaxIdleConnsPerHost": 16,
        "ResponseHeaderTimeout": 16
    }
}